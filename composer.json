{
    "name": "rokfor/rokfor-php-starter",
    "description": "Rokfor Website Boilerplate running on PHP using Slim, Jade, Sass and Grunt",
    "keywords": ["rokfor", "slim","framework","view","template","jade"],
    "homepage": "http://cloud.rokfor.ch",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Rokfor",
            "email": "inquiries@rokfor.ch",
            "homepage": "http://cloud.rokfor.ch"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/urshofer/slim-jade"
        },
        {
            "type": "vcs",
            "url": "https://github.com/urshofer/php-restclient"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "~3.0",
        "jlndk/slim-jade": "dev-pug",
        "tcdent/php-restclient": "dev-rokfor",
        "monolog/monolog": "^1.21"
    },
    "require-dev": {
        "php-console/php-console": "^3.1.3"
    },
    "suggest": {
        "php-console/php-console": "Allow sending log messages to Google Chrome, install it if you want to omit the log file setting"
    }    
}
